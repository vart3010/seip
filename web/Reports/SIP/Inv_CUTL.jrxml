	<parameter name="idCentro" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
cent.descriptionEstado as Estado,
cent.descriptionMunicipio as Municipio,
cent.descriptionParroquia as Parroquia,
cent.id as idCentro,
cent.description as Centro,
date_format(inv.fecha,'%d/%m/%Y') as Fecha,
inv.id as ObsId,
inv.cantidad as Cant,
mat.material as Material,
UPPER(inv.observations) as Observaciones

from sip_centro_inventory as inv
inner join sip_centro as cent on (inv.codigocentro=cent.codigoCentro)
inner join sip_centro_material as mat on (inv.material_id=mat.id)
where inv.deletedAt is null and cent.id=$P{idCentro}
order by fecha]]>
	</queryString>
